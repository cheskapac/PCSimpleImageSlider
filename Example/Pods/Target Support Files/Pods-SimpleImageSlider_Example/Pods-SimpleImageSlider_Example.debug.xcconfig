FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/AFNetworking" "$PODS_CONFIGURATION_BUILD_DIR/PureLayout" "$PODS_CONFIGURATION_BUILD_DIR/SimpleImageSlider" "$PODS_CONFIGURATION_BUILD_DIR/VGParallaxHeader"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/AFNetworking/AFNetworking.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/PureLayout/PureLayout.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SimpleImageSlider/SimpleImageSlider.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/VGParallaxHeader/VGParallaxHeader.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "AFNetworking" -framework "PureLayout" -framework "SimpleImageSlider" -framework "VGParallaxHeader"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
